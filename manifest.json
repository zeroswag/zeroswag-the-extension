{
    "name": "zeroswag",
    "description": "Displays a random item from a set of things.",
    "version": "0.0.0.6",
    "manifest_version": 3,
    "host_permissions" : ["http://zeroswag.mobil.ninja/"],
    "action": {
        "default_icon": {
          "16": "icon16.png"
          },
        "default_title": "zeroswag"
    },
    "background": {
        "service_worker": "icon.js"
    },
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "icons": { "16": "icon16.png",
               "48": "icon48.png",
              "128": "icon128.png" }
}
